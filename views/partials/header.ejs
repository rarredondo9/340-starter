<header id="top-header">
  <span class="siteName">
    <a href="/" title="Return to home page">CSE Motors</a>
  </span>

  <div id="tools">
    <% if (typeof accountData !== "undefined" && accountData) { %>
      <a href="/account/management" title="View your account">
        Welcome <%= accountData.account_firstname %>
      </a>
      <a href="/account/logout" title="Logout of your account">Logout</a>
    <% } else { %>
      <a href="/account/login" title="Login to your account">My Account</a>
    <% } %>
  </div>
</header>
